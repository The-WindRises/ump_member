<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.activity.health.HealthReportDetailActivity">

    <include layout="@layout/common_top2" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/len_16"
                    android:paddingRight="@dimen/len_16">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingTop="@dimen/len_16"
                        android:paddingBottom="@dimen/len_14">

                        <TextView
                            android:id="@+id/tv_name_tips"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/label_examine_name"
                            android:textColor="@color/textColor6"
                            android:textSize="@dimen/font_14" />

                        <View
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />

                        <TextView
                            android:id="@+id/tv_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="right"
                            android:textColor="@color/textColor1"
                            android:textSize="@dimen/font_14" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/bgColor4" />

                    <LinearLayout
                        android:id="@+id/ll_part"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingTop="@dimen/len_16"
                        android:paddingBottom="@dimen/len_14">

                        <TextView
                            android:id="@+id/tv_part_tips"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/label_image_part"
                            android:textColor="@color/textColor6"
                            android:textSize="@dimen/font_14" />

                        <View
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />

                        <TextView
                            android:id="@+id/tv_part"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/textColor1"
                            android:textSize="@dimen/font_14" />

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/bgColor4" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingTop="@dimen/len_16"
                        android:paddingBottom="@dimen/len_14">

                        <TextView
                            android:id="@+id/tv_organization_tips"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/label_medical_institutions"
                            android:textColor="@color/textColor6"
                            android:textSize="@dimen/font_14" />

                        <View
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />

                        <TextView
                            android:id="@+id/tv_organization"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/textColor1"
                            android:textSize="@dimen/font_14" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/bgColor4" />

                    <LinearLayout
                        android:id="@+id/ll_department"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingTop="@dimen/len_16"
                        android:paddingBottom="@dimen/len_14">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/label_administrative_office"
                            android:textColor="@color/textColor6"
                            android:textSize="@dimen/font_14" />

                        <View
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />

                        <TextView
                            android:id="@+id/tv_department"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/textColor1"
                            android:textSize="@dimen/font_14" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/bgColor4" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/len_16"
                    android:paddingRight="@dimen/len_16">

                    <TextView
                        android:id="@+id/tv_look_tips"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/len_16"
                        android:text="@string/label_examination_finding"
                        android:textColor="@color/textColor1"
                        android:textSize="@dimen/font_14" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="@dimen/len_10"
                        android:background="@color/bgColor4" />

                    <TextView
                        android:id="@+id/tv_look"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="top"
                        android:minHeight="@dimen/len_32"
                        android:textColor="@color/textColor1"
                        android:textSize="@dimen/font_12" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_result"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/len_10"
                    android:background="@color/white"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/len_16"
                    android:paddingRight="@dimen/len_16">

                    <TextView
                        android:id="@+id/tv_result_tips"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/len_16"
                        android:text="@string/label_image_result"
                        android:textColor="@color/textColor1"
                        android:textSize="@dimen/font_14" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="@dimen/len_10"
                        android:background="@color/bgColor4" />

                    <TextView
                        android:id="@+id/tv_result"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="top"
                        android:minHeight="@dimen/len_32"
                        android:textSize="@dimen/font_12" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/len_10"
                    android:background="@color/white"
                    android:orientation="vertical"
                    android:layout_marginBottom="@dimen/len_20"
                    android:paddingLeft="@dimen/len_16"
                    android:paddingRight="@dimen/len_16">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/len_16"
                            android:text="@string/label_examin_pic"
                            android:textColor="@color/textColor1"
                            android:textSize="@dimen/font_14" />

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="@dimen/len_10"
                        android:background="@color/bgColor4" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/len_20"
                        android:gravity="left"
                        android:orientation="horizontal"
                        android:paddingTop="@dimen/len_16"
                        android:paddingBottom="@dimen/len_14">

                        <ImageView
                            android:id="@+id/iv_image_1"
                            android:layout_width="@dimen/len_48"
                            android:layout_height="@dimen/len_48"
                            android:layout_marginLeft="@dimen/len_10"
                            android:src="@mipmap/icon_image_defout"
                            android:visibility="gone" />

                        <ImageView
                            android:id="@+id/iv_image_2"
                            android:layout_width="@dimen/len_48"
                            android:layout_height="@dimen/len_48"
                            android:layout_marginLeft="@dimen/len_10"
                            android:src="@mipmap/icon_image_defout"
                            android:visibility="gone" />

                        <ImageView
                            android:id="@+id/iv_image_3"
                            android:layout_width="@dimen/len_48"
                            android:layout_height="@dimen/len_48"
                            android:layout_marginLeft="@dimen/len_10"
                            android:src="@mipmap/icon_image_defout"
                            android:visibility="gone" />

                        <ImageView
                            android:id="@+id/iv_image_4"
                            android:layout_width="@dimen/len_48"
                            android:layout_height="@dimen/len_48"
                            android:layout_marginLeft="@dimen/len_10"
                            android:src="@mipmap/icon_image_defout"
                            android:visibility="gone" />

                        <ImageView
                            android:id="@+id/iv_image_5"
                            android:layout_width="@dimen/len_48"
                            android:layout_height="@dimen/len_48"
                            android:layout_marginLeft="@dimen/len_10"
                            android:src="@mipmap/icon_image_defout"
                            android:visibility="gone" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>
        </ScrollView>

        <it.swiftelink.com.common.widget.stateview.StateView
            android:id="@+id/stateView"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </FrameLayout>
</LinearLayout>